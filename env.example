# Neural DAO Environment Variables
# Copy this file to .env.local and fill in your actual values

# App Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NODE_ENV=development

# Database (Optional - for production)
DATABASE_URL=postgresql://username:password@localhost:5432/neural_dao

# Redis (Optional - for caching)
REDIS_URL=redis://localhost:6379

# Stripe Integration
STRIPE_API_KEY=sk_test_your_stripe_secret_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here
STRIPE_PUBLISHABLE_KEY=pk_test_your_publishable_key_here

# HubSpot Integration
HUBSPOT_API_KEY=pat-your_hubspot_api_key_here
HUBSPOT_PORTAL_ID=your_hubspot_portal_id_here

# Salesforce Integration
SALESFORCE_INSTANCE_URL=https://your-instance.salesforce.com
SALESFORCE_ACCESS_TOKEN=your_salesforce_access_token_here
SALESFORCE_CLIENT_ID=your_salesforce_client_id_here
SALESFORCE_CLIENT_SECRET=your_salesforce_client_secret_here

# Google Analytics Integration
GA_PROPERTY_ID=your_ga_property_id_here
GA_CREDENTIALS=your_ga_credentials_json_here

# Mixpanel Integration
MIXPANEL_PROJECT_ID=your_mixpanel_project_id_here
MIXPANEL_API_SECRET=your_mixpanel_api_secret_here

# Google Ads Integration
GOOGLE_ADS_CUSTOMER_ID=your_google_ads_customer_id_here
GOOGLE_ADS_DEVELOPER_TOKEN=your_google_ads_developer_token_here
GOOGLE_ADS_CLIENT_ID=your_google_ads_client_id_here
GOOGLE_ADS_CLIENT_SECRET=your_google_ads_client_secret_here

# Facebook Ads Integration
FACEBOOK_APP_ID=your_facebook_app_id_here
FACEBOOK_APP_SECRET=your_facebook_app_secret_here
FACEBOOK_ACCESS_TOKEN=your_facebook_access_token_here

# Email Integration (SendGrid)
SENDGRID_API_KEY=your_sendgrid_api_key_here

# Slack Integration (Optional)
SLACK_BOT_TOKEN=xoxb-your_slack_bot_token_here
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/your/webhook/url

# Security
NEXTAUTH_SECRET=your_nextauth_secret_here
NEXTAUTH_URL=http://localhost:3000

# AI/ML Configuration
OPENAI_API_KEY=your_openai_api_key_here
GOOGLE_AI_API_KEY=your_google_ai_api_key_here

# ElevenLabs Voice AI
ELEVENLABS_API_KEY=39343325a3873b0eabf32d8aa14aed5249e755f02e7b2cf16f8b5ab9b91c9dff

# Monitoring (Optional)
SENTRY_DSN=your_sentry_dsn_here
DATADOG_API_KEY=your_datadog_api_key_here

# Feature Flags
ENABLE_REAL_TIME_DATA=true
ENABLE_WEBHOOKS=true
ENABLE_ANALYTICS=true
ENABLE_NOTIFICATIONS=true
